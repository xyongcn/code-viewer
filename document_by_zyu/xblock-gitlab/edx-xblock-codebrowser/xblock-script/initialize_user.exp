#!/usr/bin/expect -f 
 set email [lindex $argv 0]  
 set pub_key [lindex $argv 1]   
 set username [lindex $argv 2]
 set ip 192.168.1.62 
 set timeout 10  

 #log in gitlab server
 spawn ssh root@$ip  
 
 expect "~#"  
 send "./create.sh $email \"$pub_key\" $username\r"
 send  "exit\r"  
 expect eof 
